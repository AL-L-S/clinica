SELECT ambulatorio_guia_id, paciente_id, convenio_id, data_cadastro, 
       operador_cadastro, data_atualizacao, operador_atualizacao, ativo, 
       data_criacao, data_unificacao, operador_unificacao, antigopaciente_id, 
       empresa_id, tipo, observacoes, data_observacoes, operador_observacoes, 
       nota_fiscal, recibo, peso, altura, pasistolica, padiastolica, 
       declaracao, operador_declaracao, data_declaracao, guiaconvenio, 
       valor_guia, via, leito, equipe_id, equipe, checado, operador_checado, 
       data_checado
  FROM ponto.tb_ambulatorio_guia
  WHERE ambulatorio_guia_id IN (38437);




SELECT agenda_exames_id, guia_id,agrupador_fisioterapia,numero_sessao, qtde_sessao, data_autorizacao, paciente_id, procedimento_tuss_id, data,
       inicio, fim, confirmado, ativo, data_cadastro, operador_cadastro, 
       
       data_atualizacao, operador_atualizacao, guia_id,agrupador_fisioterapia, valor, 
       ambulatorio_pacientetemp_id, observacoes, data_realizacao, operador_realizacao, 
       convenio_id, autorizacao, cancelada, ambulatorio_cancelamento_id, 
       observacao_cancelamento, operador_cancelamento, data_cancelamento, 
       quantidade, valor_total, medico_agenda, medico_solicitante, forma_pagamento, 
       telefonema, operador_telefonema, data_telefonema, operador_medico_agenda, 
       data_medico_agenda, operador_editar, data_editar, data_autorizacao, 
       operador_autorizacao, editarprocedimento_tuss_id, editarvalor_total, 
       editarforma_pagamento, 
       data_desbloqueio, operador_desbloqueio, bloqueado, data_unificacao, 
       operador_unificacao, antigopaciente_id, data_observacoes, operador_observacoes, 
       valor1, forma_pagamento2, valor2, forma_pagamento3, valor3, forma_pagamento4, 
       valor4, operador_faturamento, data_faturamento, desconto, faturado

  FROM ponto.tb_agenda_exames
  WHERE agenda_exames_id IN (849652) 
  AND agrupador_fisioterapia is not null
  AND (tipo = 'FISIOTERAPIA' OR tipo = 'ESPECIALIDADE')
  ORDER BY data;
